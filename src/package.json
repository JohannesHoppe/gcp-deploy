{
  "name": "@wagnermaciel/gcp-deploy",
  "version": "0.0.1",
  "description": "Deployment from the Angular CLI to Google Cloud Platform.",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && json2ts deploy/schema.json > deploy/schema.d.ts && tsc && copyfiles README.md builders.json collection.json ng-add-schema.json package.json gcp-deploy deploy/schema.json dist",
    "test": "jest"
  },
  "schematics": "./collection.json",
  "builders": "./builders.json",
  "ng-add": {
    "save": "devDependencies"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wagnermaciel/gcp-deploy.git"
  },
  "keywords": [
    "angular",
    "cli",
    "angular-cli",
    "deploy",
    "ng deploy",
    "ng-deploy",
    "gcp",
    "google-cloud",
    "google cloud"
  ],
  "author": {
    "name": "Wagner Maciel",
    "email": "wagnermaciel@google.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/wagnermaciel/gcp-deploy#readme",
  "devDependencies": {
    "@angular-devkit/architect": "^0.803.0",
    "@angular-devkit/core": "^8.3.0",
    "@angular-devkit/schematics": "^8.3.0",
    "@types/fs-extra": "^8.0.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.6.9",
    "copyfiles": "^2.1.1",
    "husky": "^3.0.7",
    "jest": "^24.8.0",
    "json-schema-to-typescript": "^7.0.0",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "@angular-devkit/architect": ">=0.800.0",
    "@angular-devkit/core": ">=8.0.0",
    "@angular-devkit/schematics": ">=8.0.0"
  },
  "dependencies": {
    "fs-extra": "^8.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
